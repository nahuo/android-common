<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="NhRippleView">
        <attr name="nh_rv_alpha" format="integer" />
        <attr name="nh_rv_framerate" format="integer" />
        <attr name="nh_rv_rippleDuration" format="integer" />
        <attr name="nh_rv_zoomDuration" format="integer" />
        <attr name="nh_rv_color" format="color" />
        <attr name="nh_rv_centered" format="boolean" />
        <attr name="nh_rv_type" format="enum">
            <enum name="simpleRipple" value="0" />
            <enum name="doubleRipple" value="1" />
            <enum name="rectangle" value="2" />
        </attr>
        <attr name="nh_rv_ripplePadding" format="dimension" />
        <attr name="nh_rv_zoom" format="boolean" />
        <attr name="nh_rv_zoomScale" format="float" />
    </declare-styleable>
</resources>